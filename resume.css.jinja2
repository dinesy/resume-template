@font-face { font-family: "Calibri"; font-display: swap; src: local("Calibri"), url("/font/Calibri.otf"), format('otf'); font-weight: normal; font-style: normal;}
@font-face { font-family: "Calibri"; font-display: swap; src: local("Calibri-Bold"), local("Calibri Bold"),url("/font/Calibri-Bold.otf"), format('otf');  font-weight: bold; font-style: normal;}
@font-face { font-family: "Calibri"; font-display: swap; src: local("Calibri-Italic"), local("Calibri Italic"),url("/font/Calibri-Italic.otf"), format('otf');  font-weight: normal; font-style: italic;}
@font-face { font-family: "Calibri"; font-display: swap; src: local("Calibri-BoldItalic"), local("Calibri Bold Italic"),url("/font/Calibri-BoldItalic.otf"), format('otf');  font-weight: bold; font-style: italic;}

body {
    font-family: Calibri, sans-serif;
}

.resume {
    margin: 0in .25in;
    font-size: 9.5pt;
    font-family: Calibri, sans-serif;
}

div.name {
    font-size: 18pt;
    font-weight: bold;
    text-align: center;
}

div.contact-info {
    text-align: center;
    font-size: 8pt;
}

div.tagline {
    text-align: center;
    font-size:  15pt;
    font-weight: bold;
    margin-top: 8pt;
    letter-spacing: 5%;
}

div.summary {
    text-align: justify;
    line-height: 1.3;
}

div.section {
    margin-top: 10px;
}

div.section-title {
    text-align: center;
    font-size: 13pt;
    font-weight: bold;
    text-transform: uppercase;

    line-height: 1.2;
    letter-spacing: 5%;
}

div.skills {
    margin-bottom: 30px;
}

.skills-group {
    line-height: 1.2;
    vertical-align: top;
}

.skills-group .group-name {
    font-weight: bold;
    text-transform: uppercase;
}

    position: absolute;
    left: 11em;
.skills-group .skills-list {
}

.skills-group .skills-list .skill-item {
    text-wrap-mode: nowrap;
}

div.employer-id {
    font-size: 12pt;
    letter-spacing: 5%;
    text-transform: uppercase;
    margin-bottom: -5pt;
}

div.employer-id .employer-name {
    font-weight: bold;
}

div.employer-id .employer-location {
    float: right;
}

div.employer-id .employer-date {
    float: right;
    padding-left: 0.25em;
    letter-spacing: 5%;
}

div.experience-employer .experience-position {
    margin-top: 2pt;
}

div.experience-employer .experience-positions {
    margin-left: 1.5em;
    margin-bottom: 1.5em;
}

div.position-id {
    font-size: 11pt;
    text-transform: uppercase;
    letter-spacing: 5%;
    margin-top: 6pt;
}

div.position-id .position-title {
    font-weight: bold;
}

div.position-id .position-date {
    //float: right;
}

div.position-details .details-group {
    margin-top: 1pt;
    margin-bottom: 4pt;
}
div.position-details .details-group .group-name {
    font-size: 11pt;
    letter-spacing: 5%;
    text-transform: uppercase;
    margin-bottom: -3pt;
}

div.position-details .details-group ul {
    margin-block-start: 0;
    margin-block-end: 0pt;
    margin-top: -1pt;
    margin-bottom: 1pt;

    //list-style-type: disc;
}

div.position-details .details-group ul li {
    margin-top: 3pt;
    margin-bottom: 1pt;
    position: relative;
    list-style: none;
    line-height: 1.1;
}

div.position-details .details-group ul li::before {
    content: "â€¢";
    position: absolute;
    left: -10px;
    top: 1;
}

div.education-item {
    text-align: center;
    font-size: 11pt;
    text-transform: uppercase;
}
